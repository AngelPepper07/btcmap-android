insert:
INSERT
INTO Place (id, type, lat, lon, timestamp, boundsMinLat, boundsMinLon, boundsMaxLat, boundsMaxLon, tags)
VALUES ?;

selectById:
SELECT *
FROM Place
WHERE id = ?;

selectBySearchString:
SELECT * FROM Place WHERE
UPPER(tags) LIKE '%' || UPPER(:searchString) || '%';

selectByBoundingBox:
SELECT * FROM Place WHERE
lat > :minLat
AND lat < :maxLat
AND lon > :minLon
AND lon < :maxLon;

selectCount:
SELECT COUNT(*)
FROM Place;

deleteAll:
DELETE
FROM Place;