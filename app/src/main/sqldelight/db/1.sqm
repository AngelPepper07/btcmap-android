import org.json.JSONObject;

CREATE TABLE Conf (
    lastSyncDateTime TEXT
);

CREATE TABLE Place (
    type TEXT NOT NULL,
    id TEXT NOT NULL,
    lat REAL NOT NULL,
    lon REAL NOT NULL,
    timestamp TEXT NOT NULL,
    boundsMinLat REAL,
    boundsMinLon REAL,
    boundsMaxLat REAl,
    boundsMaxLon REAL,
    tags TEXT AS JSONObject NOT NULL
);

CREATE TABLE Location (
    lat REAL NOT NULL,
    lon REAL NOT NULL
);