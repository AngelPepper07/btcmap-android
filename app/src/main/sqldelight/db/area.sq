CREATE TABLE area (
    id TEXT NOT NULL PRIMARY KEY,
    name TEXT NOT NULL,
    type TEXT NOT NULL,
    min_lon REAL NOT NULL,
    min_lat REAL NOT NULL,
    max_lon REAL NOT NULL,
    max_lat REAL NOT NULL
);

insertOrReplace:
INSERT OR REPLACE
INTO area(
    id,
    name,
    type,
    min_lon,
    min_lat,
    max_lon,
    max_lat
)
VALUES ?;

selectAll:
SELECT *
FROM area
ORDER BY name;

selectByid:
SELECT *
FROM area
WHERE id = ?;

deleteAll:
DELETE
FROM area;